<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <!-- Favicons -->
        <link href="{{ asset('devfolio/assets/img/favicon.png') }}" rel="icon">
        <link href="{{ asset('devfolio/assets/img/apple-touch-icon.png') }}" rel="apple-touch-icon">

        <!-- Vendor CSS Files -->
        <link href="{{ asset('devfolio/assets/vendor/ionicons/css/ionicons.min.css') }}" rel="stylesheet">
        <link href="{{ asset('devfolio/assets/vendor/animate.css/animate.min.css') }}" rel="stylesheet">
        {% include "common_css.html.twig" %}

        <!-- Template Main CSS File -->
        <link href="{{ asset('devfolio/assets/css/style.css') }}" rel="stylesheet">
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% include "common_js.html.twig" %}
        <!-- Template Main JS File -->
        <script src="{{ asset('devfolio/assets/js/main.js') }}"></script>

        {% block javascripts %}{% endblock %}
    </body>
</html>
