{% extends 'base.html.twig' %}

{% block title %}Education{% endblock %}

{% block body %}
    <h1>Education</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ education.id }}</td>
            </tr>
            <tr>
                <th>Institution</th>
                <td>{{ education.institution }}</td>
            </tr>
            <tr>
                <th>Fromyear</th>
                <td>{{ education.fromyear ? education.fromyear|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Toyear</th>
                <td>{{ education.toyear ? education.toyear|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Coursename</th>
                <td>{{ education.coursename }}</td>
            </tr>
            <tr>
                <th>Degree</th>
                <td>{{ education.degree }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ education.location }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('education_index') }}">back to list</a>

    <a href="{{ path('education_edit', {'id': education.id}) }}">edit</a>

    {{ include('education/_delete_form.html.twig') }}
{% endblock %}
