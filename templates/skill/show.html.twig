{% extends 'base.html.twig' %}

{% block title %}Skill{% endblock %}

{% block body %}
    <h1>Skill</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ skill.id }}</td>
            </tr>
            <tr>
                <th>Skillname</th>
                <td>{{ skill.skillname }}</td>
            </tr>
            <tr>
                <th>Levelofskill</th>
                <td>{{ skill.levelofskill }}</td>
            </tr>
            <tr>
                <th>Experienceofskill</th>
                <td>{{ skill.experienceofskill }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('skill_index') }}">back to list</a>

    <a href="{{ path('skill_edit', {'id': skill.id}) }}">edit</a>

    {{ include('skill/_delete_form.html.twig') }}
